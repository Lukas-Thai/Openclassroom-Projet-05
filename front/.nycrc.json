{
  "all": true,
  "include": [
    "src/**/*.ts",
    "src/**/*.js"
  ],
  "exclude": [
    "**/*.spec.ts",
    "**/*.spec.js",
    "**/node_modules/**",
    "**/cypress/**",
    "src/main.ts",
    "src/polyfills.ts",
    "src/test.ts",
    "src/environments/**"
  ],
  "extension": [
    ".ts",
    ".js"
  ],
  "report-dir": "coverage/e2e",
  "reporter": [
    "html",
    "lcov",
    "text",
    "text-summary"
  ],
  "sourceMap": true,
  "instrument": true
}
